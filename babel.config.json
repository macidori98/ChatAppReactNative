{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".ios.js", ".android.js", ".js", ".json"],
        "alias": {
          "components": "./src/components",
          "screens": "./src/screens",
          "navigation": "./src/navigation",
          "theme": "./src/theme",
          "assets": "./src/assets",
          "colors": "./src/colors",
          "common": "./src/common",
          "styles": "./src/styles",
          "types": "./src/types",
          "values": "./src/values",
          "helpers": "./src/helpers",
          "store": "./src/store",
          "translations": "./src/translations",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
