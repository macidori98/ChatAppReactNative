{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".ios.js", ".android.js", ".js", ".json"],
        "alias": {
          "components": "./src/components",
          "screens": "./src/screens",
          "navigation": "./src/navigation",
          "theme": "./src/theme"
        }
      }
    ]
  ]
}
